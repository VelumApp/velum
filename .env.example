# Local stack ports
FRONTEND_PORT=3000
POSTGRES_PORT=5432

# Database settings
POSTGRES_DB=budget
DATABASE_USERNAME=budget
DATABASE_PASSWORD=budget

# Security secrets (JWT secret should be at least 32 characters)
JWT_SECRET=replace-with-a-long-random-string
ENCRYPTION_SECRET=replace-with-a-long-random-string

# Frontend talks to backend through frontend Nginx proxy by default
VITE_API_BASE_URL=/api/v1

# Useful if you call backend directly from browser tools/apps
APP_CORS_ALLOWED_ORIGINS=http://localhost:3000